#!/bin/bash

pwd=`pwd`
path_to_script=`dirname $0`
# XXX osx native readlink doesn't support -f, gnu readlink does, but i don't want to require macports
#switcher_home=`readlink -f ${pwd}/${path_to_script}/..`
switcher_home="${pwd}/${path_to_script}/.."
echo "Switcher home = $switcher_home"

pushd ${switcher_home} > /dev/null 2>&1
java -cp build/switcher.jar com.syzygryd.Switcher /opt/syzygryd/etc/setlist.csv
popd > /dev/null 2>&1

