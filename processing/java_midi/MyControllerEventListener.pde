/* -*- mode: java; c-basic-offset: 2; indent-tabs-mode: nil -*- */

class MyControllerEventListener implements ControllerEventListener {
  void controlChange(ShortMessage event) {
    println("controlChange()");
    println("channel=" + event.getChannel() +
            " command=" + event.getCommand() +
            " data1=" + event.getData1() +
            " data2=" + event.getData2());
  }
}
